<launch>
	<!-- launches 2 nodes: FronCVNode and simulator -->
	<!-- Written by Jean-Sebastien Dery, Jan 25, McGill Robotics -->
	
 	<!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="world_name" value="$(find simulator)/worlds/simulator.world"/>
		<!-- more default parameters can be changed here -->
	</include>       

	<!-- front-end -->
	<node name="front_end" pkg="front_end" type="central_app.py" respawn="false" output="screen"/>         

	<!-- Controls systems -->
	<node name="controls" pkg="controls" type="controls"  respawn="false" output="screen"/>   
	
	<!-- CVNode front-->
	<node name="cv_Front" pkg="computer_vision" type="front_cv_node"  respawn="false" output="screen">
		<param name="image_feed/left" type="str" value="/simulator/camera1/image_raw" />
		<param name="image_feed/right" type="str" value="/simulator/camera2/image_raw" />
	</node>

	<!-- CVNode down -->
	<!-- <node name="CV_Down" pkg="computer_vision" type="DownCVNode"  respawn="false" output="screen" args="/simulator/camera3/image_raw"/> -->
</launch>
